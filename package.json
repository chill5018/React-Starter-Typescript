{
  "version": "1.0.0",
  "name": "react-starter-typescript",
  "description": "React Starter Typescript",
  "author": "Colin Hill",
  "main": "index.js",
  "private": true,
  "engines": {
    "node": ">=8.11"
  },
  "scripts": {
    "build-dev": "webpack --mode development",
    "lint": "eslint ./src --fix",
    "storybook": "start-storybook -p 6006 -c .storybook",
    "test": "jest",
    "test:coverage": "jest --watch --coverage",
    "test:watch": "jest --watch",
    "start": "serve -s dist",
    "postinstall": "yarn build",
    "dev": "webpack-dev-server --mode development --env.env=dev",
    "dev:bundleanalyze": "npm run dev -- --env.addons=bundleanalyze",
    "build": "webpack --env.env=prod --mode production",
    "build:bundleanalyze": "npm run build -- --env.addons=bundleanalyze",
    "build:bundlevisualizer": "npm run build -- --env.addons=bundlevisualizer"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-stage-2": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@storybook/addon-actions": "^4.1.13",
    "@storybook/addon-info": "^4.1.13",
    "@storybook/addon-jest": "^4.1.13",
    "@storybook/addon-knobs": "^4.1.13",
    "@storybook/addon-links": "^4.1.13",
    "@storybook/react": "^4.1.13",
    "@svgr/webpack": "^4.2.0",
    "@types/enzyme": "^3.9.0",
    "@types/jest": "^24.0.9",
    "@types/node": "^11.10.4",
    "@types/react": "^16.8.6",
    "@types/react-dom": "^16.8.2",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-test-renderer": "^16.8.1",
    "@types/storybook__react": "^4.0.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "compression-webpack-plugin": "^2.0.0",
    "copy-webpack-plugin": "^5.0.0",
    "css-loader": "^2.1.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.10.0",
    "eslint": "^5.15.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "^3.0.1",
    "husky": "^1.3.1",
    "ignore-styles": "^5.0.1",
    "image-webpack-loader": "^4.6.0",
    "jest": "^24.1.0",
    "jest-enzyme": "^7.0.1",
    "jsdom": "^13.2.0",
    "lint-staged": "^8.1.5",
    "node-sass": "^7.0.0",
    "react-addons-test-utils": "^15.6.2",
    "react-hot-loader": "^4.7.1",
    "react-test-renderer": "^16.8.3",
    "sass-loader": "^7.1.0",
    "sinon": "^7.2.6",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.2.3",
    "ts-jest": "^24.0.0",
    "ts-node": "^8.0.2",
    "typescript": "^3.3.3333",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3.1.0",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.1",
    "webpack-visualizer-plugin": "^0.1.11"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "dotenv": "^6.2.0",
    "dotenv-webpack": "^1.7.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.3",
    "react-content-loader": "^4.0.1",
    "react-dom": "^16.8.3",
    "serve": "^10.1.2",
    "ts-loader": "^5.3.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts, js,json,css,md, tsx, jsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/.jest/setupTests.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/__tests__/setup/",
      "<rootDir>/node_modules"
    ],
    "setupFiles": [
      "<rootDir>/.jest/helpers.js",
      "<rootDir>/.jest/dom.js"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/.jest/assetsTransformer.js"
    },
    "moduleFileExtensions": [
      "tsx",
      "js",
      "ts"
    ],
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "\\.(ts|tsx)$": "ts-jest",
      "^.+\\.css$": "<rootDir>/.jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/.jest/fileTransform.js"
    },
    "testRegex": "/*.spec.(ts|tsx|js)$"
  }
}
